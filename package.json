{
  "name": "macx.im",
  "version": "1.0.0",
  "description": "macx.im",
  "scripts": {
    "clean": "rm -rf dist .cache",
    "copy:public": "mkdir dist && cp src/CNAME dist/CNAME",
    "dev": "parcel src/*.html",
    "prod": "parcel build src/*.html",
    "deploy": "yarn copy:public && yarn prod && gh-pages -d dist -b master"
  },
  "repository": "https://github.com/macxim.github.io",
  "author": "Maxime Laforet",
  "license": "MIT",
  "devDependencies": {
    "autoprefixer": "^9.8.0",
    "parcel": "^1.12.4",
    "postcss": "^7.0.31",
    "tailwindcss": "^1.4.6"
  },
  "dependencies": {
    "gh-pages": "^3.0.0",
    "tailwindcss-filters": "^3.0.0"
  }
}
